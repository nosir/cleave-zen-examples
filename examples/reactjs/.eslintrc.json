{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":  [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "arrow-body-style": ["error", "as-needed"],
        "no-console": "warn"
    }
}